<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean
        class="in.spring.firstexample.Student" id="studentid">
        <property name="name" value="abhi"/>
        <property name="roll_no" value="101"/>
        <property name="email"  value="email.com"/>

    </bean>
    <bean
            class="in.spring.firstexample.Student" id="studentid2">
        <property name="name" value="jack"/>
        <property name="roll_no" value="102"/>
        <property name="email"  value="email.com2"/>

    </bean>

</beans>